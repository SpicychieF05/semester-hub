# Supabase Configuration (Required)
REACT_APP_SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Site Configuration (Required for OAuth redirects)
REACT_APP_SITE_URL=https://your-domain.vercel.app

# Build Configuration
NODE_ENV=production
GENERATE_SOURCEMAP=false

# Optional: Analytics and Performance
REACT_APP_VERCEL_ANALYTICS_ID=your_analytics_id_here

# Vercel Deployment Settings
# These are set automatically by Vercel, no need to configure manually:
# VERCEL=1
# VERCEL_URL=
# VERCEL_GIT_COMMIT_SHA=
